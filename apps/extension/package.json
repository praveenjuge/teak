{
  "name": "@teak/extension",
  "description": "Teak - Your Personal Knowledge Hub. Save, organize, and rediscover web content, text selections, and ideas effortlessly.",
  "version": "1.0.0",
  "author": "Praveen Juge (hi@praveenjuge.com)",
  "homepage": "https://teakvault.com",
  "type": "module",
  "scripts": {
    "dev": "wxt --port 3003",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "test": "bun test __tests__ --coverage --dots",
    "typecheck": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@convex-dev/better-auth": "^0.10.10",
    "@tailwindcss/vite": "^4.1.18",
    "@teak/convex": "workspace:*",
    "better-auth": "1.4.9",
    "convex": "^1.31.1",
    "lucide-react": "^0.575.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.32",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@wxt-dev/module-react": "^1.1.5",
    "typescript": "^5.9.3",
    "wxt": "^0.20.17"
  }
}
