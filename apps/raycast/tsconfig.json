{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["node", "@raycast/api"]
  },
  "include": ["raycast-env.d.ts", "src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules", "src/__tests__"]
}
